<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>JnlpDownloadServlet</servlet-name>
        <servlet-class>org.jdesktop.deployment.jnlp.servlet.JnlpDownloadServlet</servlet-class>
        <init-param>
            <param-name>jnlpSubstitutionHandlerClass</param-name>
            <param-value>org.jdesktop.wonderland.front.servlet.WonderlandSubstitutionHandler</param-value>
        </init-param>
    </servlet>
    <servlet-mapping>
        <servlet-name>JnlpDownloadServlet</servlet-name>
        <url-pattern>/app/*</url-pattern>
    </servlet-mapping>
</web-app>
